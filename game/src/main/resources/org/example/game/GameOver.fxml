<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="800" prefWidth="800" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.GameOverMenu">
    <children>
        <ImageView fitHeight="800.0" fitWidth="800.0" layoutX="-1.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@Image/EndGame.png" />
            </image></ImageView>
        <Button fx:id="restartGame" layoutX="304.0" layoutY="581.0" mnemonicParsing="false" onAction="#restart" prefHeight="70.0" prefWidth="190.0" />
        <Button fx:id="exitButton" layoutX="304.0" layoutY="686.0" mnemonicParsing="false" onAction="#exit" prefHeight="70.0" prefWidth="190.0" />
        <Button fx:id="backToMenu" layoutX="39.0" layoutY="692.0" mnemonicParsing="false" onAction="#backToMenu" prefHeight="80.0" prefWidth="80.0" />

        <Label fx:id="currentScoreLabel" layoutX="375" layoutY="330.0" prefHeight="40.0" prefWidth="300.0" text="0" textAlignment="CENTER" />

        <Label fx:id="highScore1Label" layoutX="375.0" layoutY="380.0" prefHeight="35.0" prefWidth="300.0" text="0" textAlignment="CENTER" />
        <Label fx:id="highScore2Label" layoutX="375.0" layoutY="415.0" prefHeight="35.0" prefWidth="300.0" text="0" textAlignment="CENTER" />
        <Label fx:id="highScore3Label" layoutX="375.0" layoutY="450.0" prefHeight="35.0" prefWidth="300.0" text="0" textAlignment="CENTER" />
        <Label fx:id="highScore4Label" layoutX="375.0" layoutY="485.0" prefHeight="35.0" prefWidth="300.0" text="0" textAlignment="CENTER" />
        <Label fx:id="highScore5Label" layoutX="375.0" layoutY="520.0" prefHeight="35.0" prefWidth="300.0" text="0" textAlignment="CENTER" />
    </children>
</AnchorPane>